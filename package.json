{
  "name": "capy-cloud-bot",
  "version": "1.0.0",
  "description": "Telegram bot",
  "main": "dist/app.js",
  "repository": "https://github.com/capyton/capy-cloud-bot/",
  "scripts": {
    "distribute": "yarn build-ts && node dist/app.js",
    "develop": "tsc-watch --skipLibCheck --onSuccess 'node dist/app.js'",
    "build-ts": "tsc --skipLibCheck",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.1.1",
    "@grammyjs/runner": "^1.0.4",
    "@tonconnect/sdk": "^2.0.7",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "grammy": "^1.15.3",
    "grammy-middlewares": "^1.0.11",
    "qrcode": "^1.5.1",
    "source-map-support": "^0.5.21",
    "ton-core": "^0.48.0",
    "ton-crypto": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/qrcode": "^1.5.0",
    "eslint": "^8.36.0",
    "prettier": "2.8.4",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
